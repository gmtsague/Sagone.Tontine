@model IEnumerable<Tontine.Entities.Models.CoreEtablissement>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Libelle)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Agrement)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Dossierfiscale)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Pays)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Libelle)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Agrement)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Dossierfiscale)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Pays.PaysId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Idetab">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Idetab">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Idetab">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
